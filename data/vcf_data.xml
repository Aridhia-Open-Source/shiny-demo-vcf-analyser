<TableDefinition TableName='vcf_data' Action='append'>
  <Columns>
    <Column Name='variant_id' Type='integer'/>
    <Column Name='sample_id' Type='varchar(50)'/>
    <Column Name='line_no' Type='integer'/>
    <Column Name='chrom' Type='varchar(50)'/>
    <Column Name='pos' Type='integer'/>	
    <Column Name='id' Type='varchar(60)'/>
    <Column Name='ref' Type='text'/>
    <Column Name='alt' Type='text'/>
    <Column Name='qual' Type='varchar(50)'/>	
    <Column Name='filter' Type='varchar(50)'/>	
    <Column Name='info' Type='text'/>
    
<Column Name='FORMAT' Type='text'/>

<Column Name='OTHER' Type='text'/>
</Columns>
  <Format Delimiter=',' TextQualifier='"' NullQualifier='NA' Encoding='UTF-8' Header='true' HeaderCase='lower' DateFormat='ISO, DMY'/>
  <Success RemovefromUpload='True'/>
  <Fail RemovefromUpload='False'/>
</TableDefinition>